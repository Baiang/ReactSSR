{
  "presets": [ "next/babel" ],
  "plugins": [
   [
    "styled-jsx/babel",
     { "plugins": ["styled-jsx-plugin-sass"] }
   ],
   "inline-react-svg",
   "react-html-attrs",
   ["@babel/plugin-proposal-decorators", { "legacy": true }]
 ],
 "env": {
    "development": {
      "presets": ["next/babel", "@zeit/next-typescript/babel"]
    },
    "production": {
      "presets": ["next/babel", "@zeit/next-typescript/babel"]
    },
    "test": {
      "presets": [["next/babel", { "preset-env": { "modules": "commonjs" } }], "@zeit/next-typescript/babel"]
    }
  }
}
