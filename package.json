    "styled-jsx-plugin-sass": "^0.3.0",
    "styled-jsx-postcss": "^0.2.0",
    "styled-jsx-plugin-postcss": "^1.0.0",
    "supertest": "^3.1.0",
    "ts-node": "^7.0.0",
